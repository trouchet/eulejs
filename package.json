{
  "name": "eulejs",
  "description": "Euler's diagrams are non-empty Venn's diagrams.",
  "version": "1.0.2",
  "type": "module",
  "homepage": "https://github.com/web-needle/eulejs#readme",
  "bugs": {
    "url": "https://github.com/web-needle/eulejs/issues"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:cache": "jest --clearCache",
    "test:run": "npm run test:cache && jest",
    "test:watch": "npm run test:cache && jest --watchAll --collect-coverage --coverage",
    "release": "np --any-branch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-needle/eulejs.git"
  },
  "keywords": [
    "euler",
    "venn",
    "diagram"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "devDependencies": {
    "eslint-nibble": "^8.1.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "np": "^7.6.3",
    "prettier": "^2.8.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "lodash": "^4.17.21",
    "ts-jest": "^29.0.5"
  }
}
